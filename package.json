{
  "name": "docx-tmpl",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run index.ts",
    "dev": "bun run --watch index.ts",
    "build:prod": " bun build --compile --baseline --minify --sourcemap ./index.ts ./public/* --outfile dist/docx-tmpl",
    "build:linux:arm64": " bun build --compile --target=bun-linux--arm64 --minify --sourcemap  ./index.ts ./public/* --outfile dist/docx-tmpl",
    "build:darwin:arm64": " bun build --compile --target=bun-darwin-arm64 --minify --sourcemap  ./index.ts ./public/* --outfile dist/docx-tmpl"
  },
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "easy-template-x": "^6.2.1"
  }
}
